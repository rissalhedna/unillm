{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "excludeFiles": [
    "**/*.pyc",
    "**/__pycache__/**",
    ".env",
    ".git/**",
    ".venv/**",
    "automations/**",
    "docs/**",
    "logdir/**",
    "tests/**",
    "prisma/migrations/**",
    "Dockerfile",
    "README.md",
    "pytest.ini",
    "package.json",
    "frontend/**",
    "notebooks/**",
    "scripts/**",
    "docker-compose.yml",
    "dockerfile",
    "readMe.md",
    "tests/**",
    "*.test.js",
    "*.spec.js"
  ]
}
